{
  "name": "kea-language",
  "displayName": "Kea Language",
  "description": "Syntax highlighting for the Kea programming language",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "kea",
        "aliases": ["Kea", "kea"],
        "extensions": [".kea"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kea",
        "scopeName": "source.kea",
        "path": "./syntaxes/kea.tmLanguage.json"
      }
    ]
  }
}
